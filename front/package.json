{
  "name": "sportmasters-dasboard",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "prepreview": "yarn run build",
    "preview": "vite preview",
    "lint": "eslint '*/**/*.{ts,tsx}'",
    "prelint": "tsc",
    "tsc:watch": "tsc --watch",
    "audit:dev": "npx yarn-audit-fix@latest --audit-level=moderate",
    "storybook": "start-storybook -s ./ -p 9001 -c .storybook",
    "old.prepare": "husky install",
    "preci": "rm -fr node_modules",
    "ci": "yarn install --frozen-lockfile"
  },
  "dependencies": {
    "@commitlint/config-conventional": "^15.0.0",
    "@mantine/core": "^4.1.0",
    "@mantine/hooks": "^4.1.0",
    "@mantine/notifications": "^4.1.1",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@types/uuid": "^8.3.3",
    "commitlint": "^15.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "firebase": "9.0.1",
    "formik": "^2.2.9",
    "i18next": "^21.5.5",
    "mobx": "^6.3.8",
    "mobx-react-lite": "^3.2.2",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-i18next": "^11.14.3",
    "react-icons": "^4.3.1",
    "react-router-dom": "^6.0.2",
    "uuid": "^8.3.2",
    "vite-tsconfig-paths": "^3.4.1",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@commitlint/cz-commitlint": "^15.0.0",
    "@cypress/vite-dev-server": "^2.2.2",
    "@testing-library/dom": "^8.11.3",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.0.3",
    "@types/node": "^17.0.2",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "@vitejs/plugin-legacy": "^1.6.4",
    "@vitejs/plugin-react": "^1.0.7",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "autoprefixer": "^10.4.0",
    "commitizen": "^4.2.4",
    "cypress": "^9.1.1",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^25.3.0",
    "eslint-plugin-jest-dom": "^3.9.2",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-testing-library": "^5.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.4.5",
    "lint-staged": "^12.1.3",
    "postcss": "^8.4.5",
    "postcss-import": "^14.0.2",
    "prettier": "^2.5.1",
    "sort-package-json": "^1.53.1",
    "start-server-and-test": "^1.14.0",
    "ts-jest": "^27.1.2",
    "typescript": "^4.5.4",
    "vite": "^2.8.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:ferisjuan/sportmasters-dashboard.git"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": [
        "yarn preview",
        "lint-staged"
      ]
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint '*/**/*.{ts,tsx}' --cache --fix"
  }
}
