{
    "name": "sportmasters-dasboard",
    "private": true,
    "version": "0.0.0",
    "scripts": {
        "predev": "yarn graphql:codegen",
        "dev": "vite",
        "prebuild": "yarn graphql:codegen",
        "build": "tsc && vite build",
        "prepreview": "yarn run build",
        "preview": "vite preview",
        "lint": "eslint --fix '*/**/*.{ts,tsx}'",
        "prelint": "tsc",
        "tsc:watch": "tsc --watch",
        "audit:dev": "npx yarn-audit-fix@latest --audit-level=moderate",
        "storybook": "start-storybook -s ./ -p 9001 -c .storybook",
        "old.prepare": "husky install",
        "preci": "rm -fr node_modules",
        "ci": "yarn install --frozen-lockfile",
        "graphql:codegen": "graphql-codegen --config codegen.js -r dotenv/config"
    },
    "dependencies": {
        "@mantine/core": "^4.2.7",
        "@mantine/dates": "^4.2.7",
        "@mantine/form": "^4.2.7",
        "@mantine/hooks": "^4.2.7",
        "@mantine/modals": "^4.2.7",
        "@mantine/notifications": "^4.2.7",
        "@types/react": "^18.0.9",
        "@types/react-dom": "^18.0.4",
        "@types/uuid": "^8.3.3",
        "dayjs": "^1.11.1",
        "dotenv": "^16.0.1",
        "eslint-plugin-prettier": "^4.0.0",
        "graphql": "^15.8.0",
        "graphql-request": "^4.2.0",
        "graphql-tag": "^2.12.6",
        "i18next": "^21.5.5",
        "luxon": "^2.4.0",
        "mobx": "^6.3.8",
        "mobx-react-lite": "^3.2.2",
        "react": "^18.1.0",
        "react-dom": "^18.1.0",
        "react-i18next": "^11.14.3",
        "react-icons": "^4.3.1",
        "react-query": "^3.34.19",
        "react-router-dom": "^6.0.2",
        "react-uuid": "^1.0.2",
        "uuid": "^8.3.2",
        "yup": "^0.32.11"
    },
    "devDependencies": {
        "@cypress/vite-dev-server": "^2.2.2",
        "@graphql-codegen/cli": "2.6.2",
        "@graphql-codegen/introspection": "2.1.1",
        "@graphql-codegen/typescript-operations": "2.4.0",
        "@graphql-codegen/typescript-react-query": "^3.5.12",
        "@graphql-codegen/typescript": "2.4.11",
        "@testing-library/dom": "^8.11.3",
        "@testing-library/jest-dom": "^5.16.1",
        "@testing-library/react-hooks": "^7.0.2",
        "@testing-library/react": "^12.1.2",
        "@testing-library/user-event": "^13.5.0",
        "@types/jest": "^27.0.3",
        "@types/luxon": "^2.3.2",
        "@types/node": "^17.0.2",
        "@types/react-cookies": "^0.1.0",
        "@typescript-eslint/eslint-plugin": "^5.8.0",
        "@typescript-eslint/parser": "^5.8.0",
        "@vitejs/plugin-legacy": "^1.6.4",
        "@vitejs/plugin-react-refresh": "^1.3.6",
        "@vitejs/plugin-react": "^1.0.7",
        "autoprefixer": "^10.4.0",
        "commitizen": "^4.2.4",
        "commitlint": "^15.0.0",
        "cypress": "^9.1.1",
        "eslint-config-prettier": "^8.3.0",
        "eslint-import-resolver-typescript": "^2.5.0",
        "eslint-plugin-cypress": "^2.12.1",
        "eslint-plugin-import": "^2.25.3",
        "eslint-plugin-jest-dom": "^3.9.2",
