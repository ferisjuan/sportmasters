    "prettier": "^2.2.0",
    "storybook": "^6.3.8",
    "tsc": "^2.0.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint '*/**/*.{ts,tsx}' --cache --fix"
  },
